SELECT I.SUMMARY, I2.NAME
FROM issues I JOIN issueslists i2 on I.ISSUESLIST_ID = i2.ID;

SELECT I.SUMMARY, U.FIRSTNAME, U.LASTNAME
FROM issues I JOIN USERS U ON I.USER_ID_ASSIGNEDTO = U.ID;

SELECT U.FIRSTNAME, U.LASTNAME, COUNT(*) AS QUANTITY_TASKS
FROM USERS U JOIN issues I ON U.ID = I.USER_ID_ASSIGNEDTO
GROUP BY U.ID
